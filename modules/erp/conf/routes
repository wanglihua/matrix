GET  /erp/brand                    ErpBrand.Index
POST /erp/brand/data/list          ErpBrand.ListData
POST /erp/brand/data/detail        ErpBrand.DetailData
POST /erp/brand/save               ErpBrand.Save
POST /erp/brand/delete             ErpBrand.Delete

GET  /erp/stock                    ErpStock.Index
POST /erp/stock/data/list          ErpStock.ListData
POST /erp/stock/data/detail        ErpStock.DetailData
POST /erp/stock/save               ErpStock.Save
POST /erp/stock/delete             ErpStock.Delete

GET  /erp/storage/loc              ErpStorageLoc.Index
POST /erp/storage/loc/data/list    ErpStorageLoc.ListData
POST /erp/storage/loc/data/detail  ErpStorageLoc.DetailData
POST /erp/storage/loc/save         ErpStorageLoc.Save
POST /erp/storage/loc/delete       ErpStorageLoc.Delete



GET  /erp/supplier                    ErpSupplier.Index
POST /erp/supplier/data/list          ErpSupplier.ListData
POST /erp/supplier/data/detail        ErpSupplier.DetailData
POST /erp/supplier/save               ErpSupplier.Save
POST /erp/supplier/delete             ErpSupplier.Delete
